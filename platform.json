  "engines": {
    "platformio": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/platformio/platform-maxim32.git"
  },
  "version": "2.4.1",
  "packageRepositories": [
    "https://dl.bintray.com/platformio/dl-packages/manifest.json",
    "http://dl.platformio.org/packages/manifest.json"
  ],
  "frameworks": {
    "mbed": {
      "package": "framework-mbed",
