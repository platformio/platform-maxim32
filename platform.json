{
  "name": "maxim32",
  "title": "Maxim 32",
  "description": "Maxim's microcontrollers provide low-power, efficient, and secure solutions for challenging embedded applications. Maxim's processors embed cutting-edge technologies to secure data and intellectual property, proven analog circuitry for real-world applications, and battery-conserving low power operation.",
  "homepage": "https://www.maximintegrated.com/en/products/digital/microcontrollers.html",
  "license": "Apache-2.0",
  "keywords": [
      "dev-platform",
      "ultra-low-power",
      "ARM",
      "Cortex-M",
      "Maxim Integrated",
      "MAX32"
  ],
  "engines": {
    "platformio": "^6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/platformio/platform-maxim32.git"
  },
  "version": "4.0.0",
  "frameworks": {
    "mbed": {
      "package": "framework-mbed",
      "script": "builder/frameworks/mbed.py"
    }
  },
  "packages": {
    "toolchain-gccarmnoneeabi": {
      "type": "toolchain",
      "owner": "platformio",
      "version": "~1.80201.0"
    },
    "framework-mbed": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~6.60600.0",
      "optionalVersions": ["~6.51506.0"]
    },
    "tool-pyocd": {
      "type": "debugger",
      "optional": true,
      "owner": "platformio",
      "version": "~1.2900.0"
    },
    "tool-jlink": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "^1.63208.0"
    }
  }
}
