{
  "build": {
    "cpu": "cortex-m3",
    "f_cpu": "24000000L",
    "mcu": "max32600"
  },
  "debug": {
    "tools": {
      "cmsis-dap": {
        "onboard": true,
        "server": {
          "arguments": [
            "pyocd-gdbserver.py",
            "-t",
            "max32600mbed"
          ],
          "executable": "$PYTHONEXE",
          "package": "tool-pyocd"
        }
      }
    }
  },
  "frameworks": [
    "mbed"
  ],
  "name": "Maxim ARM mbed Enabled Development Platform for MAX32600",
  "upload": {
    "maximum_ram_size": 32768,
    "maximum_size": 262144,
    "protocol": "mbed",
    "protocols": [
      "cmsis-dap",
      "mbed"
    ]
  },
  "url": "https://developer.mbed.org/platforms/MAX32600mbed/",
  "vendor": "Maxim"
}
