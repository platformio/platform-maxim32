{
  "build": {
    "cpu": "cortex-m3",
    "f_cpu": "24000000L",
    "mcu": "max32610"
  },
  "connectivity": [
    "bluetooth"
  ],
  "debug": {
    "tools": {
      "cmsis-dap": {
        "server": {
          "arguments": [
            "pyocd-gdbserver.py",
            "-t",
            "maxwsnenv"
          ],
          "executable": "$PYTHONEXE",
          "package": "tool-pyocd",
          "ready_pattern": "GDB server started on port"
        }
      }
    }
  },
  "frameworks": [
    "mbed"
  ],
  "name": "Maxim Wireless Sensor Node Demonstrator",
  "upload": {
    "maximum_ram_size": 32768,
    "maximum_size": 262144,
    "protocol": "mbed",
    "protocols": [
      "cmsis-dap",
      "mbed"
    ]
  },
  "url": "https://developer.mbed.org/platforms/MAXWSNENV/",
  "vendor": "Maxim"
}
